x = int(input("Iveskite reiksme x: "))

y = int(input("Iveskite reiksme y: "))

operation = input("Pasirinkite veiksma (+, -, *, /: ")

if operation == "+":
    print(x + y)
elif operation == "-":
    print(x - y)
elif operation == "*":
    print(x * y)
elif operation == "/":
    print(x / y)
else:
    print("Jus nepasirinkote jokio slaiciavimo veiksmo.")
